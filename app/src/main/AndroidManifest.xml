<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.electrices">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/electrices_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/electrices_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Web_scraper"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name=".Activities.RadarChartActivity" />
        <activity android:name=".Activities.PieChartActivityTest" />
        <activity android:name=".Activities.DevicesActivity" />
        <activity android:name=".Activities.BarChartActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.electrices.Activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>